<div class='theme-container [ grid ]'>

    {% for theme in item.themes %}
        
        <div class='theme-swatch [ col large-fifth medium-half ]'>
      
            <div  class='swatch__top ml-0 mr-0 ' style='padding-bottom:0; '>
                <div class='{{theme.name}} mt-l pb-m' style='border-top-style: solid;
    border-top-width: 10px;'></div>
            </div>

            <div class='swatch__bottom '>
            
                {{theme.name}}
      
            </div>
            
        </div>

    {% endfor %}

</div>