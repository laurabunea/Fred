{
    "api": {
        "type": "pill",
        "data": []
    },
    "examples": [
        {
            "title": "Time",
            "example": {
                "type": "traffic/time",
                "data":"45 min"
            }
        },
        {
            "title": "Roadworks",
            "example": {
                "type": "traffic/roadworks",
                "data": { "street": "Tristram Ave.", "action": "Road closed" }
            }
        },
        {
            "title": "Warning",
            "example": {
                "type": "traffic/warning",
                "data": { "street": "Fanshawe St.", "action": "Delays" }
            }
        },
        {
            "title": "Everything OK",
            "example": {
                "type": "traffic/healthy",
                "data": { "street": "Fanshawe St.", "action": "No delays" }
            }
        },
        {
            "title": "Pills can also be used without label text",
            "example": {
                "type": "p",
                "data": [
                    {
                        "type": "traffic/warning",
                        "data": { "action": "2" }
                    },
                    {
                        "type": "traffic/roadworks",
                        "data": { "action": "4" }
                    }
                ]

            }
        }
    ]
}